{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "6.16.3",
    "@umijs/preset-react": "1.x",
    "ahooks": "2.10.1",
    "antd": "4.15.1",
    "dayjs": "^1.10.4",
    "framer-motion": "^4.1.10",
    "nprogress": "^0.2.0",
    "storejs": "^1.1.0",
    "styled-components": "5.2.3",
    "swiper": "6.5.7",
    "umi": "3.4.8"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/styled-components": "5.1.9",
    "@umijs/test": "3.4.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "4.2.4",
    "yorkie": "2.0.0"
  }
}
